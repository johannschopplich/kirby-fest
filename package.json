{
  "name": "kirby-fest",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@7.11.0",
  "description": "A Collection of TypeScript types for the Kirby CMS",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/kirby-fest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/kirby-fest.git"
  },
  "bugs": "https://github.com/johannschopplich/kirby-fest/issues",
  "keywords": [
    "kirby",
    "kirby-cms",
    "getkirby",
    "typescript",
    "kirbyql",
    "kql"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts"
    }
  },
  "types": "./index.d.ts",
  "files": [
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "eslint \"**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "release": "bumpp --commit --push --tag",
    "test": "tsc --noEmit && tsd"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "tsd": "^0.24.1",
    "typescript": "^4.8.3"
  }
}
